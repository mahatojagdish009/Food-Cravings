# Environment-specific configuration for staging
NEXT_PUBLIC_APP_ENV=staging
NEXT_PUBLIC_APP_VERSION=1.0.0-staging
NEXT_PUBLIC_ANALYTICS_ENABLED=false

# API Configuration
NEXT_PUBLIC_API_BASE_URL=https://web-staging.vercel.app
NEXT_PUBLIC_HEALTH_CHECK_URL=https://web-staging.vercel.app/healthz

# Feature Flags
NEXT_PUBLIC_ENABLE_CHAT=true
NEXT_PUBLIC_ENABLE_ENHANCED_CHAT=false
NEXT_PUBLIC_ENABLE_DASHBOARD=true

# Performance Settings
NEXT_PUBLIC_MAX_SEARCH_RESULTS=10
NEXT_PUBLIC_SEARCH_TIMEOUT=5000